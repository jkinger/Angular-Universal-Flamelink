{
  "name": "my-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ssr": "ng build --prod && npm run build:functions && npm run build:hosting",
    "serve:ssr": "node dist/my-app/server.js",
    "webpack:ssr": "webpack --config webpack.server.config.js",
    "build:hosting": "cp -r ./dist/my-app/* ./public && rm ./public/index.html",
    "build:functions": "ng run my-app:server && npm run webpack:ssr && npm run --prefix functions build",
    "build:deploy": "firebase deploy"
  },
  "engines": {
    "node": "8"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.3",
    "@angular/common": "^7.1.3",
    "@angular/compiler": "^7.1.3",
    "@angular/core": "^7.1.3",
    "@angular/fire": "^5.1.1",
    "@angular/forms": "^7.1.3",
    "@angular/http": "^7.1.3",
    "@angular/platform-browser": "^7.1.3",
    "@angular/platform-browser-dynamic": "^7.1.3",
    "@angular/platform-server": "^7.1.3",
    "@angular/router": "^7.1.3",
    "@nguniversal/express-engine": "^7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "core-js": "^2.5.7",
    "dynamic-import-ponyfill": "^0.1.0",
    "firebase": "^5.3.1",
    "flamelink": "^0.19.6",
    "node-fetch": "^2.2.0",
    "rxjs": "^6.2.2",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.3",
    "@angular/compiler-cli": "^7.1.3",
    "@angular/language-service": "^7.1.3",
    "@types/jasmine": "~3.3.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "express": "^4.16.3",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.3",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-loader": "^5.3.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "3.1.6",
    "webpack-cli": "^3.1.0",
    "ws": "^6.0.0",
    "xmlhttprequest": "^1.8.0"
  }
}
